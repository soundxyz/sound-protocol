{
  "name": "@soundxyz/protocol",
  "version": "2.0.0",
  "author": "soundxyz",
  "main": "typechain/index.ts",
  "homepage": "https://github.com/soundxyz/sound-protocol#readme",
  "scripts": {
    "build": "forge build",
    "test": "forge test",
    "test:gas": "forge test --gas-report",
    "solhint": "solhint -f table contracts/**/*.sol",
    "slither": "slither . --config-file slither.config.json",
    "prepare": "husky install",
    "postinstall": "forge install",
    "prettier:write": "prettier --write contracts/**/*.sol tests/**/*.sol",
    "typechain": "rm -rf out/**/*.t.sol && npx typechain --discriminate-types --target ethers-v5 --out-dir typechain 'out/**/*[!metadata].json'",
    "release:canary": "(node scripts/canary-release.js && pnpm -r publish --access public --no-git-checks --tag alpha) || echo Skipping Canary...",
    "prepack": "bob-tsm build.ts",
    "postpublish": "gh-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soundxyz/sound-protocol.git"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@changesets/apply-release-plan": "^6.0.4",
    "@changesets/assemble-release-plan": "^5.2.1",
    "@changesets/cli": "^2.23.2",
    "@changesets/config": "^2.1.1",
    "@changesets/read": "^0.5.7",
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@manypkg/get-packages": "^1.1.3",
    "@pnpm/exportable-manifest": "^3.1.2",
    "@pnpm/types": "^8.5.0",
    "@typechain/ethers-v5": "^10.1.0",
    "bob-ts": "^4.0.0",
    "bob-tsm": "^1.1.1",
    "danger": "^11.1.1",
    "esbuild": "^0.15.5",
    "ethers": "^5.6.9",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-solidity": "1.0.0-dev.23",
    "semver": "^7.3.7",
    "solhint": "3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-scope": "^4.0.3",
    "eslint-utils": "^1.4.3",
    "eslint-visitor-keys": "^1.3.0",
    "prettier-linter-helpers": "^1.0.0"
  }
}
